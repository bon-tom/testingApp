    <ActionBlock>
      <Name value="handler_dataContext1_OnUserChange" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if form.complex1.type.enumValue() = "series" then
	form.editBool = true
else
	if form.complex1.seasons.hasValue() then
		form.confirm = apiobjects.page1.confirm("This action will delete all seasons and episodes. Are you sure about that?")
		if form.confirm = true then
			form.complex1.seasons.clear()
			form.editBool = false
		else
			form.complex1.type.clear()
		endif		
	else
		form.editBool = false
	endif
endif
]]></Script>
