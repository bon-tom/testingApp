    <ActionBlock>
      <Name value="initeditRecord" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[system.console("Tohle jsou data pro edit ve formuláři edit" +form.recordForEdit)
system.console(tostring(form.recordForEdit.isTouched()) )

if apiobjects.breadcrumbsCtrl1.count() = 2 then
	apiobjects.breadcrumbsCtrl1.addBreadcrumb("edit", "Edit", "", "", false)
else
	apiobjects.breadcrumbsCtrl1.addBreadcrumb("detail", "Movie Detail", "", "", true)
	apiobjects.breadcrumbsCtrl1.addBreadcrumb("edit", "Edit", "", "", false)
endif


form.complex1.fromJson(form.recordForEdit)
this.form.id = form.complex1.id
system.console("Id na návrat"+this.form.id)]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="SaveToServerII" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.saveConfirm = apiobjects.page1.confirm("Do you really want to save all changes?")
form.attrForBusy = true

if form.saveConfirm = true then
	var response = apiobjects.serverForMovies.postAsync("update", form.complex1.toJson())


	if (response.isError()) then
		system.consoleError(response.getErrorMessage())
	else 
		form.postResult = response.getJsonString()
		form.ContentType = response.getHeader("Content-Type")
		//navigace na hlavní formulář vynechána, raději postupný UPDATE
    	//navigation.main.movies1.go("")
	endif
	form.attrForBusy = false
	// úspěšný POSTING?
	//navigace na hlavní formulář vynechána, raději postupný UPDATE
	navigation.main.moviesList.go()

else 
//navigace na hlavní formulář
    form.attrForBusy = false
	navigation.main.moviesList.go()
endif]]></Script>
