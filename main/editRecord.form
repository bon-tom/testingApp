        <Script><![CDATA[form.backConfirm = apiobjects.page1.confirm("This action will cancel all your changes. Do you want that?")


system.console(form.id)

if form.backConfirm = true then
	if params.type = "breadcrumbsClick" then
		if params.data = "main" then		
			navigation.main.moviesList.go()
		endif
	endif

	if params.type = "breadcrumbsClick" then
		if params.data = "detail" then		
			navigation.main.movieDetail.go(this.form.id)
		endif
	endif	
endif

]]></Script>
